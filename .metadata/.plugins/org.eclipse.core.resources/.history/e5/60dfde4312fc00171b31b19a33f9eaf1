
public class roupe {

	public static void main(String[] args) {
		   for(int a=0; a<3; a++){
			   System.out.println(a);
		   }
		   System.out.println("<br>");

           for(int b=1; b<10; b++){
        	   System.out.println(b);
           }
           System.out.println("<br>");

           for (int c=3; c<10; c++){
        	   System.out.println(c);
           }
           System.out.println("<br>");


	}

}
