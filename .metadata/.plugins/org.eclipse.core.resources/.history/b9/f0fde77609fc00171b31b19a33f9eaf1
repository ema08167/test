
public class sequence {

	public static void main(String[] args) {
		// 配列
          String [] name=new String[3];
          name[0]="田中";
          name[1]="高橋";
          name[2]="斉藤";
          System.out.println(name [0]);

          String[] name1={"田中","高橋","斉藤"};
             System.out.println(name1[0]);
	}

}
