package during;

public class Durant {

	public static void main(String[] args) {
		int a=1;
		while(a<=5){
			System.out.println(a);
			a++;
		}System.out.println("\n");

        int b=0;
        while(b<10){
        	System.out.println(b);
        	b++;
        }System.out.println("\n");

        int c=5;
        while(c<10){
        	System.out.println(c);
        	c++;
        }System.out.println("\n");

        int d=5;
        while(d>0){
        	System.out.println(d);
        	d--;
        }System.out.println("\n");

        int e=10;
        while(e>=5){
        	System.out.println(e--);
        }System.out.println("\n");

        int f=1;
        while(f<=10){
        	System.out.println(f);
        	f+=2;
        }System.out.println("\n");

        int g=1;
        while(g<=10){
        	System.out.println(g);
        	g+=3;
        }System.out.println("\n");

	}

}
